<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	/* Add CSS here. See the GWT docs on UI Binder for more details */
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%" ui:field="mainPanel">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell width="100%">
				<g:HorizontalPanel width="100%" spacing="4"
					height="387px" borderWidth="2" ui:field="hp">
					<g:Cell width="33%" height="100%">
						<g:CaptionPanel captionText="Classes" width=""
							height="360px">
							<g:ListBox visibleItemCount="10" width="100%" height="340px"
								ui:field="classListBox" />
						</g:CaptionPanel>
					</g:Cell>
					<g:Cell width="33%">
						<g:VerticalPanel width="100%" height="100%">
							<g:Cell width="50%">
								<g:CaptionPanel captionText="Class Info" width=""
									height="168px" ui:field="classInfo">
									<g:Grid width="100%" height="147px">
										<g:row>
											<g:customCell>
												<g:Label text="Id:" width="60" horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="classIdTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="User Id:" width=" "
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="userClassIdTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Nb Inst:" horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="nbInstTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label />
											</g:customCell>
											<g:customCell>
												<g:Button text="Refresh" ui:field="refreshB" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
							<g:Cell width="50%">
								<g:CaptionPanel captionText="Instance Info"
									width="" height="168px" ui:field="instanceInfo">
									<g:Grid width="100%" height="147px">
										<g:row>
											<g:customCell>
												<g:Label text="Class Name:" width="60"
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="classNameTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Alias:" width="60"
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="aliasTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Full Path:" width=""
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="fullPathTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Nb Att:" width=" "
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="nbAttTB" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
						</g:VerticalPanel>
					</g:Cell>
					<g:Cell width="33%">
						<g:CaptionPanel captionText="Instances" width=""
							height="360px">
							<g:ListBox visibleItemCount="5" width="100%" height="340px"
								ui:field="instanceListBox" />
						</g:CaptionPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
			<g:Cell width="100%">
				<g:HorizontalPanel width="100%" height="387px"
					borderWidth="2">
					<g:Cell width="33%" height="">
						<g:VerticalPanel width="100%" height="">
							<g:Cell width="100%" height="50%">
								<g:CaptionPanel captionText="Info" width=""
									height="216px" ui:field="info">
									<g:Grid width="100%" height="147px">
										<g:row>
											<g:customCell>
												<g:Label text="Explorator:" width="" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Button text="^^" ui:field="upButton" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:ListBox visibleItemCount="5" width="100%"
													height="116px" ui:field="childrenListBox" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:InlineLabel text="New InlineLabel"
													ui:field="messageLabel" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
						</g:VerticalPanel>
					</g:Cell>
					<g:Cell width="33%">
						<g:VerticalPanel width="100%" height="">
							<g:Cell height="">
								<g:CaptionPanel captionText="DataBase Info"
									width="" height="200px" ui:field="dataBaseInfo">
									<g:Grid width="100%" height="100%">
										<g:row>
											<g:customCell>
												<g:Label text="Size:" width="" horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" width="" ui:field="sizeTB" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Nb Classes:" width=""
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="nbClassesTB"
													width="" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Nb Formula:" width=""
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="nbFormulaTB"
													width="" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Nb Instances:" width=""
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="nbInstancesTB"
													width="" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Label text="Scs Path:" width=""
													horizontalAlignment="ALIGN_RIGHT" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="scsPathTB"
													width="" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>

							<g:Cell height="">
								<g:CaptionPanel captionText="DataBase request"
									width="" height="50px">
									<g:Grid width="" height="">
										<g:row>
											<g:customCell>
												<g:Label text="Instance Name:" width="" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="false" ui:field="queryTB"
													width="" />
											</g:customCell>
											<g:customCell>
												<g:Label text="Address:" width="" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="false" ui:field="queryAddressTB"
													width="100%" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
							<g:Cell height="">
								<g:Grid width="100%" height="">
									<g:row>
										<g:customCell>
											<g:Button text="Query" ui:field="queryButton" width="" />
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:Cell>
							<g:Cell height="">
								<g:Grid width="100%" height="">
									<g:row>
										<g:customCell>
											<g:ListBox visibleItemCount="5" width="100%"
												height="100%" ui:field="queryListBox" />
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:Cell>
						</g:VerticalPanel>
					</g:Cell>
					<g:Cell width="33%" height="">
						<g:VerticalPanel width="" height="">
							<g:Cell height="33%">
								<g:CaptionPanel captionText="Attributes info"
									width="" height="155px">
									<g:Grid width="100%" height="">
										<g:row>
											<g:customCell>
												<g:ListBox visibleItemCount="5" width="100%"
													height="100px" ui:field="attributeListBox" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Button text="Read current value of selected item"
													ui:field="readButton" width="100%" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
							<g:Cell width="33%" height="100%">
								<g:Grid width="100%" height="">
									<g:row>
										<g:customCell>
											<g:Button text="Add value" ui:field="addValueButton"
												width="" />
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="type" ui:field="stringValue">String</g:RadioButton>
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="type" ui:field="intValue">Int</g:RadioButton>
										</g:customCell>
										<g:customCell>
											<g:RadioButton name="type" ui:field="floatValue">Float</g:RadioButton>
										</g:customCell>
										<g:customCell>
											<g:TextBox readOnly="false" ui:field="typedValueTB"
												width="100%" />
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:Cell>
							<g:Cell width="33%" height="100%">
								<g:CaptionPanel captionText="Formulas info"
									width="" height="75px">
									<g:Grid width="100%" height="">
										<g:row>
											<g:customCell>
												<g:Label text="Formula:" width="" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="true" ui:field="formulaTB"
													width="100%" />
											</g:customCell>
										</g:row>
										<g:row>
											<g:customCell>
												<g:Button text="Add Formula" ui:field="addButton"
													width="" />
											</g:customCell>
											<g:customCell>
												<g:TextBox readOnly="false" ui:field="setFormulaTB"
													width="100%" />
											</g:customCell>
										</g:row>
									</g:Grid>
								</g:CaptionPanel>
							</g:Cell>
							<g:Cell width="33%" height="100%">
								<g:Grid width="100%" height="">
									<g:row>
										<g:customCell>
											<g:TextBox readOnly="false"
												value="Enter an address (:SITE1:B001:F000:CCTV)" ui:field="setAddressTB"
												width="100%" />
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:Cell>
							<g:Cell width="33%" height="100%">
								<g:Grid width="100%" height="">
									<g:row>
										<g:customCell>
											<g:Button text="Get CE" ui:field="getCEButton"
												width="" />
										</g:customCell>
										<g:customCell>
											<g:TextBox readOnly="true" ui:field="CEMode" width="100%" />
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:Cell>
						</g:VerticalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 