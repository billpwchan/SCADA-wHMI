<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<module rename-to='Common'>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard' />
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  
  <!-- For Arba translation and Right to Left layout-->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.ChromeRTL'/> -->
  
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/>   -->
  
  <!-- Inherit the UiBinder module.                               -->
  <inherits name="com.google.gwt.uibinder.UiBinder"/>
  
  <!-- Inherit the Debug module.                               -->
  <inherits name='com.google.gwt.user.Debug' /> 
  <inherits name="com.google.gwt.logging.Logging"/>

  <!-- Other module inherits                                      -->
  <inherits name='com.thalesgroup.scadagen.whmi.uiutil.uilogger.UILogger' />
  <inherits name='com.thalesgroup.scadagen.whmi.uiutil.uiutil.UIUtil' />

  <inherits name='com.thalesgroup.scadagen.whmi.uinamecard.uinamecard.UINameCard' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uievent.uievent.UIEvent' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.config.config.Config' />
  <inherits name='com.thalesgroup.scadagen.whmi.config.configenv.ConfigEnv' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.translation.translationmgr.TranslationMgr' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uitask.uitask.UITask' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uiwidget.uiwidget.UIWidget' />
  <inherits name='com.thalesgroup.scadagen.whmi.uiwidget.uiwidgetgeneric.UIWidgetGeneric' />
  <inherits name='com.thalesgroup.scadagen.whmi.uiwidget.uiwidgetmgr.UIWidgetMgr' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uidialog.uidialog.UIDialog' />
  <inherits name='com.thalesgroup.scadagen.whmi.uidialog.uidialogmgr.UIDialogMgr' />
  
  <inherits name='com.thalesgroup.scadagen.wrapper.widgetcontroller.WidgetController' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.common.Container' />

  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />

  <!-- To change the default logLevel -->
  <set-property name="gwt.logging.logLevel" value="SEVERE"/>
  <!-- To enable logging -->
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <!-- To disable a default Handler -->
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
  <!-- To disable a popup Handler -->
  <set-property name="gwt.logging.popupHandler" value="DISABLED" />
  <!-- Remote Logging Enable -->
  <set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />

  <set-property name="user.agent" value="gecko1_8,safari,ie9" /> 

</module>
