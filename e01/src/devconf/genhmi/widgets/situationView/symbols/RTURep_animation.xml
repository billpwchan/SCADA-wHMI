<?xml version="1.0" encoding="utf-8"?>
<animationConfiguration xmlns:ns2="http://www.thalesgroup.com/hv/mwt/conf/common" xmlns="http://www.thalesgroup.com/hv/mwt/conf/situationview/animation">
  <animationRules id="RTURep_animation">
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="comState"></source>
      <sourceType>INT</sourceType>
      <svgRefAnimation defaultValue="rtu_OUS" nodeId="comState.value_node">
        <enumerationBinding>
          <item inputValue="2" value="rtu_OUM"></item>
          <item inputValue="1" value="rtu_OUS"></item>
          <item inputValue="3" value="rtu_INS"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="spyMode"></source>
      <sourceType>INT</sourceType>
      <svgRefAnimation defaultValue="rtu_Spy" nodeId="spyMode.value_node">
        <enumerationBinding>
          <item inputValue="5" value="rtu_Spy"></item>
          <item inputValue="6" value="rtu_NoSpy"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:computedStatusDataSourceType" statusComputerName="com.thalesgroup.scadasoft.stresstest.calculated.widget.WidgetGlobalState" name="WidgetGlobalState"></source>
      <sourceType>STRING</sourceType>
      <svgRefAnimation defaultValue="rtu_NotConnected" nodeId="WidgetGlobalState_node">
        <enumerationBinding>
          <item inputValue="5" value="rtu_NotConnected"></item>
          <item inputValue="3" value="rtu_Invalid"></item>
          <item inputValue="4" value="rtu_OK_NotAck"></item>
          <item inputValue="2" value="rtu_NOK_NotAck"></item>
          <item inputValue="0" value="rtu_OK"></item>
          <item inputValue="1" value="rtu_NOK"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="eqpName"></source>
      <sourceType>STRING</sourceType>
      <textAnimation i18n="false" defaultValue="" nodeId="RTURep_eqp_name_eqpNameDT"></textAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="needack" />
      <sourceType>INT</sourceType>
      <blinkAnimation defaultValue="false">
        <rangeBinding>
          <range from="1" to="2147483647" value="true" />
        </rangeBinding>
      </blinkAnimation>
    </animationRule>
  </animationRules>
</animationConfiguration>
