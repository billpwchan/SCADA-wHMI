  <inherits name='com.thalesgroup.scadagen.whmi.uipanel.uipanelstatusbar.UIPanelStatusBar' />
  <inherits name='com.thalesgroup.scadagen.whmi.uipanel.uipaneltoolbar.UIPanelToolBar' />
  <inherits name='com.thalesgroup.scadagen.whmi.uipanel.uipanelviewlayout.UIPanelViewLayout' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uiview.uiview.UIView' />
  <inherits name='com.thalesgroup.scadagen.whmi.uiview.uiviewmgr.UIViewMgr' />
