# Create a link of binary on the main panel

if [ "$SYSTEMFAMILY" = "win32" ]
then
	where=$BINDIR/bin
	cible=`cygpath -w "$where"`
	truecible=$cible'\'$1
	bureau=C:\\WINNT\\Profiles\\scadadev\\Bureau
        script=`cygpath -w "$CONFDIR/bin/shortcut.vbs"`
        link=$bureau'\'$1.lnk
	cscript //nologo $script $link $truecible
fi
	
