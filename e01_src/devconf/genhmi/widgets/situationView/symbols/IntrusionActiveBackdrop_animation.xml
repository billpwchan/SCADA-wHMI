<?xml version="1.0" encoding="utf-8"?>
<animationConfiguration xmlns:ns2="http://www.thalesgroup.com/hv/mwt/conf/common" xmlns="http://www.thalesgroup.com/hv/mwt/conf/situationview/animation">
  <animationRules id="IntrusionActiveBackdrop_animation">
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:computedStatusDataSourceType" statusComputerName="com.thalesgroup.scadasoft.stresstest.calculated.widget.IntrusionColorChooser" name="IntrusionColorChooser"></source>
      <sourceType>STRING</sourceType>
      <strokeColorAnimation defaultValue="#000000"></strokeColorAnimation>
      <strokeWidthAnimation defaultValue="3.0" invalidValue="3.0">
        <enumerationBinding>
          <item inputValue="0" value="3.0"></item>
        </enumerationBinding>
      </strokeWidthAnimation>
      <fillColorAnimation defaultValue="orange" invalidValue="orange">
        <enumerationBinding>
          <item inputValue="" value="orange"></item>
        </enumerationBinding>
      </fillColorAnimation>
      <fillOpacityAnimation defaultValue="0.25" invalidValue="0.25">
        <enumerationBinding>
          <item inputValue="0" value="0.25"></item>
        </enumerationBinding>
      </fillOpacityAnimation>
    </animationRule>
  </animationRules>
</animationConfiguration>
