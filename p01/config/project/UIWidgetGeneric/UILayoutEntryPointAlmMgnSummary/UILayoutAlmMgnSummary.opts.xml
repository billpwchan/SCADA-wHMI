<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<header key="header">
		<EventBusName>almmgnsummary</EventBusName>
		<EventBusScope>local</EventBusScope>
		<InitDelayMS>1000</InitDelayMS>
	</header>
	
	<actionset key="init">
		<OperationString1>set_default_widget_control</OperationString1>
		<OperationString2>set_default_widget_cssswitchview</OperationString2>
		<OperationString3>set_default_widget_cssselection</OperationString3>
	</actionset>
	
	<actionset key="init_delay">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>set_default_widget_filter</OperationString1>
	</actionset>
	
	<action key="set_default_widget_control">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetAlmMgnControl</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_cssswitchview">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UILayoutAlmMgnCSSSwitch</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_cssselection">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetAlmMgnCSSSelection</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_filter">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetAlmMgnFilter</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
</data-set>