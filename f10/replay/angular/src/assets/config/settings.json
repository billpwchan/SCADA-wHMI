{
    "i18n": {
      "default_lang": "en"
      , "resolve_by_browser_lang": true
      , "use_culture_lang": true
      , "resolve_by_browser_cookie": false
      , "use_cookie_name": "iscs.webapp.Lang"
    }
    , "AppComponent": {
      "title": "Replay"
    }
	, "RecordComponent": {
	  "columnDefs": [
	    {
	      headerName: 'Name',
          field: 'fileName',
          width: 500
	    }
	  ]
	}
	, "RecordSearchComponent": {
	  "dateFormat: "yy-mm-dd"
	  , "hourFormat": "24"
	  , "firstDayOfWeek": 0
	}
	, "ReplayActionComponent": {
	  "speeds": [ 1, 2, 5, 10 , 20 ]
	  , "btn1Caption: "&#60;"
	  , "btn2Caption: "&#52;"
	  , "btn1Action: {
	    "type": "REST"
		, "url": "http://localhost:18080/scs/service/ReplayComponent/Start"
		, "params: [
		  "speed": "&speed"
		]
	  }
	  , "btn2Action: {
	    "type": "REST"
		, "url": "http://localhost:18080/scs/service/ReplayComponent/Stop"
		, "params": []
	  }
	}
    , "ScadagenReplayService": {
        "conn_addr": "http://localhost:18080"
    }
}