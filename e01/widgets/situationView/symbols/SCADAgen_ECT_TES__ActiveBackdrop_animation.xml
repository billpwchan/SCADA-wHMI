<?xml version="1.0" encoding="utf-8"?>
<animationConfiguration xmlns:ns2="http://www.thalesgroup.com/hv/mwt/conf/common" xmlns="http://www.thalesgroup.com/hv/mwt/conf/situationview/animation">
  <animationRules id="SCADAgen_ECT_TES__ActiveBackdrop_animation">
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="validity"></source>
      <sourceType>INT</sourceType>
      <strokeColorAnimation defaultValue="black"></strokeColorAnimation>
      <strokeWidthAnimation defaultValue="3.0" invalidValue="3.0">
        <enumerationBinding>
          <item inputValue="0" value="3.0"></item>
        </enumerationBinding>
      </strokeWidthAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:computedStatusDataSourceType" statusComputerName="IntMapping" name="IntMapping"></source>
      <sourceType>STRING</sourceType>
      <fillColorAnimation defaultValue="#999999"></fillColorAnimation>
    </animationRule>
  </animationRules>
</animationConfiguration>
