##################################################################
#                                                                #
# configurateurClient.prop : fichier de configuration du client  #
#                                                                #
# nota: toutes les valeurs de propriétés indiquant un répertoire #
#       doivent être terminées par /                             #
##################################################################

######################################
# Nom du serveur RMI
######################################
server.rmi.name=StressTest_1_2_2
server.rmi.host=127.0.0.1
server.rmi.port=2001

######################################
# Activation animator
######################################
animator.init=false

######################################################
#   pour gestion de plusieurs écrans
######################################################
# Nombre de moniteurs connectés à la carte graphique
framework.util.screennumber=1
# Résolution de la carte graphique
framework.util.xresolution=1280
framework.util.yresolution=1024

############################################################
#   pour le positionnement des écrans de l'éditeur graphique
############################################################
animator.xposition=10%
animator.yposition=30%
animator.screennumber=1

############################################################
# marge de la zone imprimable
############################################################
print.margin=40

##########################################################
#   Appel d'un script externe pour la création des groupes
#   d'abonnement à l'export2scada
#   Définir les propriétés pour activer l'appel
##########################################################

##################################################
# Internationalisation
##################################################
# define the locale
#internationalisation.langue=fr
#internationalisation.location=FR
internationalisation.langue=en
internationalisation.location=GB
internationalisation.variante=

#####################################################
# Chargement du fichier de proprietes scsconfClient
#####################################################
directory.conf=${application.home}/conf/
property.file.scsconf=${directory.conf}/scsconfClient.prop