<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<header key="header">
		<EventBusName>inputtablesummary</EventBusName>
		<EventBusScope>local</EventBusScope>
		<InitDelayMS>1000</InitDelayMS>
	</header>
	
	<actionset key="init">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>set_default_widget_dpcmocontrol</OperationString1>
		<OperationString2>set_default_widget_dpcsscontrol</OperationString2>
		<OperationString3>set_default_widget_cssswitchcontrol</OperationString3>
		<OperationString4>set_default_widget_cssswitchview</OperationString4>
		<OperationString5>set_default_widget_cssselection</OperationString5>
	</actionset>
	
	<actionset key="init_delay">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>set_default_widget_filter</OperationString1>
	</actionset>
	
	<action key="set_default_widget_dpcmocontrol">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetInputTableManualOverrideControl</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_dpcsscontrol">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetInputTableScanSuspendControl</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>

	<action key="set_default_widget_cssselection">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetInputTableCSSSelection</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_cssswitchcontrol">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UILayoutInputTableCSSSwitchControl</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_cssswitchview">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UILayoutInputTableCSSSwitchView</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>
	
	<action key="set_default_widget_filter">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetInputTableFilter</OperationElement>
		<OperationString1>set_default_action</OperationString1>
	</action>

</data-set>