<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
   
  <SCOPE name= "HILC_CONF">
    <SCOPE name= "BDD_NODE"><!-- BDD PROJECT SPECIFIC NODE -->
      <!-- Roots -->
      <CONF key = "HILC_ROOTS_NUMBER">2</CONF>
      <SCOPE name= "HILC_ROOTS">
          <CONF key = "1">PWR</CONF>
          <CONF key = "2">TUN</CONF>
      </SCOPE>

      <!--DCI -->
      <CONF key = "HILC_DCI_NODE_NAME">:dci</CONF>
      <CONF key = "HILC_ACI_NODE_NAME">:aci</CONF><!-- A VERIFIER -->
      <CONF key = "HILC_SIO_NODE_NAME">:sio</CONF>
      <CONF key = "HILC_DIO_NODE_NAME">:dio</CONF>
      <CONF key = "HILC_AIO_NODE_NAME">:aio</CONF>
      
      <!-- SEPARATOR -->
      <CONF key = "HILC_TAG_ALIAS">&lt;alias&gt;</CONF><!--OK -->
            
      <!-- ATTRIBUTE -->
      <CONF key = "HILC_ATTR_UNIVNAME">.UNIVNAME</CONF><!--OK -->
      <CONF key = "HILC_ATTR_VALUE">.value</CONF><!--OK -->

    </SCOPE>
    
    <SCOPE name= "PARAM"> <!-- HILC PARAMETERS -->
      <!-- MAX SESSION NUMBER -->
	  <CONF key = "HILC_MAX_SESSIONS">10</CONF>
	  
      <!-- TIMER -->
      <CONF key = "HILC_TIMER_DELAY">15000</CONF>
      
      <!-- MESSAGE -->
	  <!-- Utilise uniquement avec les SIO
      <CONF key = "HILC_MESSAGE_LOCALE">en_US</CONF>  
      <CONF key = "HILC_MESSAGE_FORMAT_TYPE">1</CONF>
	  -->
      <!-- COMMANDE ID -->
	  <!-- Utilise uniquement avec les SIO
      <CONF key = "HILC_PREP_CMD_ID">129</CONF> 
      <CONF key = "HILC_CONF_CMD_ID">126</CONF> 
	  -->
    
      <!-- HILC CLASS -->
      <CONF key = "HILC_CLASS_NUMBER">1</CONF>
      <SCOPE name= "HILC_CLASS">
	      <!-- TPS - LPS -->
	      <CONF key = "1">BasicEquipment</CONF>
      </SCOPE>
	  <!-- ScsTypes optional additional post filtering on scstype attribut -->
      <CONF key = "HILC_SCSTYPES_NUMBER">5</CONF>
      <SCOPE name= "HILC_SCSTYPES">
	      <!-- TPS - LPS -->
	      <CONF key = "1">LusailIcbType</CONF>
	      <CONF key = "2">LusailCbType</CONF>
	      <CONF key = "3">LusailTcbType</CONF>
	      <CONF key = "4">LusailTcbCbType</CONF>
	      <!-- TVS -->
	      <CONF key = "5">LusTvfType</CONF>
      </SCOPE>

      <!-- A utiliser dans les affaires avec un etat Session avant l'etat preparation 
      <CONF key = "HILC_SESSIONSTATE">-</CONF>
	  -->
      <!-- A utiliser dans les affaires ou une DCI couvre la preparation et la confirmation 
      <CONF key = "HILC_CURRENTSTEP">-</CONF>
	  -->
      <!-- A utiliser dans les affaires avec 1 DCI pour preparation et 1 pour confirmation -->
      <CONF key = "HILC_PREPSTATE">-AckSel</CONF>
      <CONF key = "HILC_CONFSTATE">-AckExe</CONF>     
      
      <!--SIO CMD -->
      <!-- BHE: Pour plus de coherence (les index du tableau sont HILC_PREPARATION_STEP_NUMBER, ect..)
           on va desormais utiliser HILC_NUMBER_OF_STEP a la place de HILC_SIO_TYPE_NUMBER
		   car on a a priori le meme nb de steps partout
      -->
	  <!-- Pas utilise (lie a #define USE_SIO)
      <CONF key = "HILC_SIO_ENTERSESSION_NAME">NotUsed</CONF>
      <CONF key = "HILC_SIO_PREPARATION_NAME">NotUsed</CONF>
      <CONF key = "HILC_SIO_CONFIRMATION_NAME">NotUsed</CONF>
      <CONF key = "HILC_SIO_CANCEL_NAME">NotUsed</CONF>
      -->
	  
      <!--DIO CMD -->
      <!-- BHE: Pour plus de coherence (les index du tableau sont HILC_PREPARATION_STEP_NUMBER, ect..)
           on va desormais utiliser HILC_NUMBER_OF_STEP a la place HILC_DIO_TYPE_NUMBER
      -->
	  <!-- Pas utilise : lié à USE_SESSION_EQPT
      <CONF key = "HILC_DIO_ENTERSESSION_NAME">NotUsed</CONF>
	  -->
      <CONF key = "HILC_DIO_PREPARATION_NAME">-CmdSel</CONF>
      <CONF key = "HILC_DIO_CONFIRMATION_NAME">-CmdExe</CONF>
      
      <!--AIO CMD -->
      <!-- BHE: Pour plus de coherence (les index du tableau sont HILC_PREPARATION_STEP_NUMBER, ect..)
           on va desormais utiliser HILC_NUMBER_OF_STEP a la place de HILC_AIO_TYPE_NUMBER
      -->      
	  <!-- Pas utilise : lié à USE_SESSION_EQPT
      <CONF key = "HILC_AIO_ENTERSESSION_NAME">NotUsed</CONF>
	  -->
      <CONF key = "HILC_AIO_PREPARATION_NAME">-CmdSel</CONF>
      <CONF key = "HILC_AIO_CONFIRMATION_NAME">-CmdExe</CONF>

      <!-- STEP NUMBER DEFINITION -->
      <CONF key = "HILC_NUMBER_OF_STEP">2</CONF><!--OK -->
	  <!-- Pas utilise (lie a #define USE_SESSION_EQPT)
      <CONF key = "HILC_ENTERSESSION_STEP_NUMBER">3</CONF>
	  -->
      <CONF key = "HILC_PREPARATION_STEP_NUMBER">0</CONF><!--OK -->
      <CONF key = "HILC_CONFIRMATION_STEP_NUMBER">1</CONF><!--OK -->
      
      <!-- ATTRIBUT NUMBER DEFINITION -->
      <!--DCI SUBSCRIPTION-->
      <CONF key = "HILC_ATT_NUMBER">2</CONF>
      <!-- Mettre un nombre superieur a HILC_ATT_NUMBER pour ceux que l'on utilisera pas -->
      <!-- A utiliser dans les affaires avec un etat Session avant l'etat preparation 
      <CONF key = "HILC_SESSIONSTATE_NUMBER">3</CONF>
	  -->
      <!-- A utiliser dans les affaires ou une DCI couvre la preparation et la confirmation
      <CONF key = "HILC_CURRENTSTEP_NUMBER">2</CONF>
	  -->
      <!-- A utiliser dans les affaires avec 1 DCI pour preparation et 1 pour confirmation -->
      <CONF key = "HILC_PREPSTATE_NUMBER">0</CONF><!--OK -->
      <CONF key = "HILC_CONFSTATE_NUMBER">1</CONF><!--OK -->
    </SCOPE>
    
    <SCOPE name= "ALA_EXT">
      <CONF key = "HILC_K_ALA_EXTERN_01">1</CONF>
    </SCOPE>
    
    <SCOPE name= "STATE_PATTERN">    

      <!-- END OF PREPARATION WAITING FOR USER CONFIRMATION STEP-->
      <CONF key = "HILC_WAITING_STEP_NAME">State_WaitingConf</CONF> <!-- OK -->
      
      <!-- INIT COND -->
	  <!-- un #define USE_SESSION_EQPT sera maintenant utilise (pour couverture code du cas standard)
      <CONF key = "HILC_INITSESSION_COND_ON">0</CONF>
      <CONF key = "HILC_INITSESSION_COND_VALUE">0</CONF>
	  -->
	  <!-- un #define USE_FALLBACK sera maintenant utilise
      <CONF key = "HILC_VERIFPREPCOND_TYPE">1</CONF>
      -->
      <!--DCI ModeFallBack-->
	  <!-- pas utilise actuellement (lié à #define USE_FALLBACK)
      <CONF key = "HILC_FALLBACK_CMDNAME">FallBack</CONF>
      <CONF key = "HILC_FALLBACK_DCINAME">_2000-ModeFallBack</CONF>
      <CONF key = "HILC_FALLBACK_CLASS">PMI</CONF>
      <CONF key = "HILC_FALLBACK_EQPT">CBI_1</CONF>
      <CONF key = "HILC_FALLBACK_COND_VALUE">1</CONF>
      -->
      
      <!-- STATE TRANSITION COND -->
       <!-- STATE ENTERSESSION : non utilise
      <SCOPE name= "STATE_ENTERSESSION">
          <CONF key = "HILC_STATE_ENTERSESSION_ATT1_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_ENTERSESSION_ATT2_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_ENTERSESSION_ATT1_ERROR_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_ENTERSESSION_ATT2_ERROR_CONDVALNUMBER">0</CONF>
      </SCOPE>
	  -->
            <!-- STATE PREPARATION -->
      <SCOPE name= "STATE_PREP">
          <CONF key = "HILC_STATE_PREP_ATT1_CONDVALNUMBER">1</CONF><!-- A VERIFIER SI ON PREND EN COMPTE D AUTRES ATTRIBUTS DANS LA TRANSITION-->
          <CONF key = "HILC_STATE_PREP_ATT1_CONDVAL1">1</CONF>  <!-- OK -->
          <CONF key = "HILC_STATE_PREP_ATT2_CONDVALNUMBER">0</CONF> <!-- OK -->
          <CONF key = "HILC_STATE_PREP_ATT1_ERROR_CONDVALNUMBER">2</CONF> <!-- OK -->
          <CONF key = "HILC_STATE_PREP_ATT1_ERROR_CONDVAL1">2</CONF> <!-- OK -->
          <CONF key = "HILC_STATE_PREP_ATT1_ERROR_CONDVAL2">3</CONF> <!-- OK -->
          <CONF key = "HILC_STATE_PREP_ATT2_ERROR_CONDVALNUMBER">0</CONF>
      </SCOPE>
            <!-- STATE WAITING CONFIRMATION -->
      <SCOPE name= "STATE_WAITINGCONF">
          <CONF key = "HILC_STATE_WAITINGCONF_ATT1_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_WAITINGCONF_ATT2_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_WAITINGCONF_ATT1_ERROR_CONDVALNUMBER">2</CONF>
          <CONF key = "HILC_STATE_WAITINGCONF_ATT1_ERROR_CONDVAL1">2</CONF>
          <CONF key = "HILC_STATE_WAITINGCONF_ATT1_ERROR_CONDVAL2">3</CONF>
          <CONF key = "HILC_STATE_WAITINGCONF_ATT2_ERROR_CONDVALNUMBER">0</CONF>
      </SCOPE>
            <!-- STATE CONFIRMATION -->
      <SCOPE name= "STATE_CONF">
          <CONF key = "HILC_STATE_CONF_ATT1_CONDVALNUMBER">1</CONF>
          <CONF key = "HILC_STATE_CONF_ATT1_CONDVAL1">1</CONF>
          <CONF key = "HILC_STATE_CONF_ATT2_CONDVALNUMBER">0</CONF>
          <CONF key = "HILC_STATE_CONF_ATT1_ERROR_CONDVALNUMBER">2</CONF>
          <CONF key = "HILC_STATE_CONF_ATT1_ERROR_CONDVAL1">2</CONF>
          <CONF key = "HILC_STATE_CONF_ATT1_ERROR_CONDVAL2">3</CONF>
          <CONF key = "HILC_STATE_CONF_ATT2_ERROR_CONDVALNUMBER">0</CONF>
      </SCOPE>	  
    </SCOPE>
  </SCOPE>
</root>
