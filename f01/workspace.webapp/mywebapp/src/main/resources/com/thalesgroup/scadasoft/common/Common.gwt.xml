<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.json.JSON" />
    <!-- Add gwt-log support -->
    <!-- The application is compiled once with the DEBUG log level and once with the INFO log level
         By default the application start with the DEBUG log level to change it modify the log level
         into the html start page.
     -->
    <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />
    
    <extend-property name="log_level" values="DEBUG,INFO,WARN"/> 

    <!-- Turn off the `DivLogger` -->
    <set-property name="log_DivLogger" value="DISABLED" />
    <!-- Console logger -->
    <set-property name="log_ConsoleLogger" value="ENABLED" />
    <!-- Log on the server side : uncomment to enable-->
    <!-- <inherits name="com.allen_sauer.gwt.log.gwt-log-RemoteLogger" /> -->
        
    <!-- <set-property name="user.agent" value="gecko1_8,opera,ie9,safari"/> -->
    <set-property name="user.agent" value="gecko1_8,safari,ie9"/>
	
    <!--
     <set-configuration-property name="CssResource.style" value="pretty"/>
    -->
    <inherits name='com.thalesgroup.hypervisor.mwt.core.webapp.core.ui.Ui'/>
    
    <!-- Add the option to win time at gwt compilation -->
    <!-- Do not put the option for production beacause it uses of cpu time -->
    <collapse-all-properties />
</module>
