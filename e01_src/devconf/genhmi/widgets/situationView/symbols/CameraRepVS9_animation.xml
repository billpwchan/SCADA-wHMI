<?xml version="1.0" encoding="utf-8"?>
<animationConfiguration xmlns:ns2="http://www.thalesgroup.com/hv/mwt/conf/common" xmlns="http://www.thalesgroup.com/hv/mwt/conf/situationview/animation">
  <animationRules id="CameraRepVS9_animation">
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="tagstatus"></source>
      <sourceType>INT</sourceType>
      <svgRefAnimation defaultValue="empty" nodeId="tagstatus.value_node">
        <enumerationBinding>
          <item inputValue="256" value="cam_locked"></item>
          <item inputValue="512" value="cam_workOrder"></item>
          <item inputValue="0" value="empty"></item>
          <item inputValue="768" value="cam_lockeAndwork"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:computedStatusDataSourceType" statusComputerName="com.thalesgroup.scadasoft.stresstest.calculated.widget.WidgetInhibSynthesis" name="WidgetInhibSynthesis"></source>
      <sourceType>STRING</sourceType>
      <svgRefAnimation defaultValue="empty2" nodeId="WidgetInhibSynthesis_node">
        <enumerationBinding>
          <item inputValue="1" value="cam_overriden"></item>
          <item inputValue="0" value="empty2"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:computedStatusDataSourceType" statusComputerName="com.thalesgroup.scadasoft.stresstest.calculated.widget.WidgetGlobalState" name="WidgetGlobalState"></source>
      <sourceType>STRING</sourceType>
      <svgRefAnimation defaultValue="cam_NotConnected" nodeId="WidgetGlobalState_node">
        <enumerationBinding>
          <item inputValue="0" value="cam_OK"></item>
          <item inputValue="4" value="cam_OK_NotAck"></item>
          <item inputValue="1" value="cam_NOK"></item>
          <item inputValue="2" value="cam_NOK_NotAck"></item>
          <item inputValue="3" value="cam_Invalid"></item>
          <item inputValue="5" value="cam_NotConnected"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="recstate"></source>
      <sourceType>INT</sourceType>
      <svgRefAnimation defaultValue="cam_InRecording" nodeId="recstate.value_node">
        <enumerationBinding>
          <item inputValue="0" value="cam_NotInRecording"></item>
          <item inputValue="1" value="cam_InRecording"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="state"></source>
      <sourceType>INT</sourceType>
      <svgRefAnimation defaultValue="cameraStopOk_Aspect" nodeId="state.value_node">
        <enumerationBinding>
          <item inputValue="1" value="cameraStopOk_Aspect"></item>
          <item inputValue="0" value="camInvalid_Aspect"></item>
        </enumerationBinding>
      </svgRefAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="eqpName"></source>
      <sourceType>STRING</sourceType>
      <textAnimation i18n="false" defaultValue="" nodeId="CameraRepVS9_eqp_name_eqpNameDT"></textAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="angle"></source>
      <sourceType>FLOAT</sourceType>
      <numberAnimation i18n="false" template="{}" numberFormat="" defaultValue="" nodeId="CameraRepVS9_AngleAspect_camAngleDT"></numberAnimation>
    </animationRule>
    <animationRule>
      <source xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:statusDataSourceType" name="needack" />
      <sourceType>INT</sourceType>
      <blinkAnimation defaultValue="false">
        <rangeBinding>
          <range from="1" to="2147483647" value="true" />
        </rangeBinding>
      </blinkAnimation>
    </animationRule>
  </animationRules>
</animationConfiguration>
