	<!-- Open command on ICB class -->
    <SCOPE name= "TEST_1">
      <SCOPE name= "CMD_ARG">      
        <CONF key = "EQPT">PWRTPSTPS01ICB1_1CB</CONF>
        <CONF key = "EQPT_TYPE">CBR</CONF>
        <CONF key = "CMD_NAME">Open</CONF>
        <CONF key = "CMD_TYPE">2</CONF><!-- 1 for SIO / 2 for DIO / 3 for AIO -->
        <CONF key = "CMD_VALUE">1</CONF>
      </SCOPE>
	  <CONF key = "EXPECT_TRACE_PATTERN">---+prepareCmd+equipmentAlias=PWRTPSTPS01ICB1_1CB+equipmentType=CBR+commandName=Open+State_Prep Send Preparation Command OK+State_WaitingConf+---+confirmCmd+State_Conf+State_Conf Send Confirmation Command OK+State_Final+SessionManager::deleteSession OK</CONF> <!-- + is converted to end of line -->
