<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<actionset key="init">
	</actionset>
	
	<actionset key="set_default_action">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>set_auto_down</OperationString1>
		<OperationString2>set_manu_disable</OperationString2>
	</actionset>
	
	<actionset key="auto">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>select_auto</OperationString1>
	</actionset>
	
	<actionset key="manu">
		<OperationType>actionsetkey</OperationType>
		<OperationString1>select_manu</OperationString1>
	</actionset>
	
	<actionset key="set_auto_down">
		<OperationString1>set_auto_down</OperationString1>
	</actionset>

	<action key="set_auto_down">
		<OperationType>action</OperationType>
		<OperationAction>widget</OperationAction>
		<OperationString1>SetWidgetStatus</OperationString1>
		<OperationString2>auto</OperationString2>
		<OperationString3>Down</OperationString3>
	</action>
	
	<actionset key="set_manu_down">
		<OperationString1>set_manu_down</OperationString1>
	</actionset>
	
	<actionset key="set_clean_disable">
		<OperationString1>set_clean_disable</OperationString1>
	</actionset>
	
	<!-- disable the manu in the FAS first submission, simple soc -->
	<action key="set_manu_disable">
		<OperationType>action</OperationType>
		<OperationAction>widget</OperationAction>
		<OperationString1>SetWidgetStatus</OperationString1>
		<OperationString2>manu</OperationString2>
		<OperationString3>Disable</OperationString3>
	</action>

	<action key="set_manu_down">
		<OperationType>action</OperationType>
		<OperationAction>widget</OperationAction>
		<OperationString1>SetWidgetStatus</OperationString1>
		<OperationString2>auto</OperationString2>
		<OperationString3>Down</OperationString3>
	</action>
	
	<action key="select_auto">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetSocControl</OperationElement>
		<OperationString1>RadioBoxSelected</OperationString1>
		<OperationString2>auto</OperationString2>
	</action>
	
	<action key="select_manu">
		<SenderOperation>event</SenderOperation>
		<OperationType>actionsetkey</OperationType>
		<OperationElement>UIWidgetSocControl</OperationElement>
		<OperationString1>RadioBoxSelected</OperationString1>
		<OperationString2>manu</OperationString2>
	</action>

</data-set>