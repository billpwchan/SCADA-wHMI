<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<header key="header">
		<rows>1</rows>
		<cols>4</cols>
		<rootPanel>HorizontalPanel</rootPanel>
		<rootCss>project-UIWidgetDpcTagControl-panel-inner</rootCss>
	</header>
	
	<option key="0|0">
		<widget>Button</widget>
		<label>&amp;project_button_tag</label>
		<css>project-UIWidgetDpcTagControl-button-0-0</css>
		<element>set</element>
		
		<cssContainer>project-UIWidgetDpcTagControl-container-0-0</cssContainer>
		
		<cssDown>project-UIWidgetDpcTagControlcontainer-button-0-0-down</cssDown>
		<cssDisable>project-UIWidgetDpcTagControl-button-0-0-disable</cssDisable>
		<tooltip>Tag</tooltip>
		<tooltipDown>Set Activate</tooltipDown>
		<tooltipDisable>Set Disabled</tooltipDisable>
		
	</option>
	
	
	<option key="0|1">
		<widget>TextBox</widget>
		<label></label>
		<css>project-UIWidgetDpcTagControlcontainer-textbox-0-2</css>
		<element>txtvalue</element>
		<maxlength>16</maxlength>
		<placeholder>&amp;dpctag_placeholder_ptw_comment</placeholder>
		<cssContainer>project-UIWidgetDpcTagControl-container-0-2</cssContainer>
		
	</option>
	
	<option key="0|2">
		<widget>Button</widget>
		<label>&amp;project_button_untag</label>
		<css>project-UIWidgetDpcTagControl-button-0-1</css>
		<element>unset</element>
		
		<cssContainer>project-UIWidgetDpcTagControl-container-0-1</cssContainer>
		
		<cssDown>project-UIWidgetDpcTagControl-button-0-1-down</cssDown>
		<cssDisable>project-UIWidgetDpcTagControl-button-0-1-disable</cssDisable>
		<tooltip>Un-Tag</tooltip>
		<tooltipDown>UnSet Activate</tooltipDown>
		<tooltipDisable>UnSet Disabled</tooltipDisable>

	</option>
	
	<option key="0|3">
		<widget>Button</widget>
		<label>&amp;project_button_apply</label>
		<css>project-UIWidgetDpcTagControl-button-0-2</css>
		<element>apply</element>
		
		<cssContainer>project-UIWidgetDpcTagControl-container-0-2</cssContainer>
		
		<cssDown>project-UIWidgetDpcTagControl-button-0-2-down</cssDown>
		<cssDisable>project-UIWidgetDpcTagControl-button-0-2-disable</cssDisable>
		<tooltip>Apply</tooltip>
		<tooltipDown>Apply Activate</tooltipDown>
		<tooltipDisable>Apply Disabled</tooltipDisable>
	</option>
	
</data-set>