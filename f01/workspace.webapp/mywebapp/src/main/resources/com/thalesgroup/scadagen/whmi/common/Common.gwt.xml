  <inherits name='com.thalesgroup.scadagen.whmi.uitask.uitasktitle.UITaskTitle' />
  
  <inherits name='com.thalesgroup.scadagen.whmi.uipanel.uipanel.UIPanel' />
  <inherits name='com.thalesgroup.scadagen.whmi.uipanel.uipanelmgr.UIPanelMgr' />
