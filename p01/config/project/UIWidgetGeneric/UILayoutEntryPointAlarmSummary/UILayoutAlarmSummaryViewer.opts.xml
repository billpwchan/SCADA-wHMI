<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<header key="header">
		<ScsOlsListElement>UILayoutAlarmSummaryViewerScsOlsListPanel</ScsOlsListElement>
		
		<!--  DebugID to the Target Generic Data Grid, Printer will using it to walkthough the GDD in the DOM -->
		<PrintDataDebugId>gwt-debug-UILayoutAlarmSummaryViewerScsOlsListPanel</PrintDataDebugId>
		<!-- Columns Header Display in the CSS, it support the translation; More a column should be split by ','-->
		<PrintDataColumns>&amp;scsalarmList_state_name,&amp;scsalarmList_datetime_name,&amp;scsalarmList_area_name,&amp;scsalarmList_funccat_name,&amp;scsalarmList_sourceID_name,&amp;scsalarmList_alarmText_name,&amp;scsalarmList_priority_name</PrintDataColumns>
		<!-- Columns index will be export to the CSV; More a column should be split by ','-->
		<PrintDataIndexs>1,2,3,4,5,6,7</PrintDataIndexs>
		<!-- CSV File name will be export to -->
		<PrintDataAttachement>AlarmList.csv</PrintDataAttachement>
		
		<PrintDataStart>0</PrintDataStart>
		<PrintDataLength>8000</PrintDataLength>
		
		<PrintDataReceviedWait>6000</PrintDataReceviedWait>
		<PrintDataWalkthoughWait>4000</PrintDataWalkthoughWait>
	</header>
	
