<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<header key="header">
		<rows>3</rows>
		<cols>2</cols>
		<rootPanel>FlexTable</rootPanel>
		<rootCss>project-gwt-flextable-login-button</rootCss>
	</header>
	
	<option key="0|0">
		<widget>InlineLabel</widget>
		<label>Operator:</label>
		<css>project-gwt-inlinelabel-login-info-0-0</css>
	</option>
	<option key="0|1">
		<widget>TextBox</widget>
		<label></label>
		<css>project-gwt-textbox-login-info-0-1</css>
		<element>name</element>
		<maxlength>16</maxlength>
	</option>
	
	<option key="1|0">
		<widget>InlineLabel</widget>
		<label>Profile:</label>
		<css>project-gwt-inlinelabel-login-info-1-0</css>
	</option>
	<option key="1|1">
		<widget>ListBox</widget>
		<label></label>
		<css>project-gwt-listbox-login-info-1-1</css>
		<element>profile</element>
		<visibleitemcount>1</visibleitemcount>
	</option>
	
	<option key="2|0">
		<widget>InlineLabel</widget>
		<label>Password:</label>
		<css>project-gwt-inlinelabel-login-info-2-0</css>
	</option>
	<option key="2|1">
		<widget>PasswordTextBox</widget>
		<label></label>
		<css>project-gwt-textbox-login-info-2-1</css>
		<element>password</element>
		<maxlength>16</maxlength>
	</option>
	
</data-set>