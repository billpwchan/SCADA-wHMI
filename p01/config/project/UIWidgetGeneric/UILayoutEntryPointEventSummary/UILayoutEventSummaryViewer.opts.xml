<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<data-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<header key="header">
		<ScsOlsListElement>UILayoutEventSummaryViewerScsOlsListPanel</ScsOlsListElement>
		
		<!--  DebugID to the Target Generic Data Grid, Printer will using it to walkthough the GDD in the DOM -->
		<PrintDataDebugId>gwt-debug-UILayoutEventSummaryViewerScsOlsListPanel</PrintDataDebugId>
		<!-- Columns Header Display in the CSS, it support the translation; More a column should be split by ','-->
		<PrintDataColumns>&amp;scsalarmList_datetime_name,&amp;scsalarmList_area_name,&amp;scsalarmList_funccat_name,&amp;scsalarmList_sourceID_name,&amp;scsalarmList_alarmText_name,&amp;scsalarmList_priority_name</PrintDataColumns>
		<!-- Columns index will be export to the CSV; More a column should be split by ','-->
		<PrintDataIndexs>0,1,2,3,4,5</PrintDataIndexs>
		<!-- CSV File name will be export to -->
		<PrintDataAttachement>EventList.csv</PrintDataAttachement>
	</header>
	
	<!-- Begin of life -->
	<actionset key="init">
	</actionset>
	
	<actionset key="from_uilayoutsummary_init">
		<OperationType>actionsetkey</OperationType>
	</actionset>
	
	<actionset key="from_uilayoutsummary_envup">
		<OperationType>actionsetkey</OperationType>
	</actionset>
		
	<actionset key="from_uilayoutsummary_envdown">
		<OperationType>actionsetkey</OperationType>
	</actionset>
	
	<actionset key="from_uilayoutsummary_terminate">
		<OperationType>actionsetkey</OperationType>
	</actionset>
	<!-- End of life -->

	<actionset key="FilterAdded">
		<OperationString1>FilterAdded</OperationString1>
	</actionset>
	
	<actionset key="FilterRemoved">
		<OperationString1>FilterRemoved</OperationString1>
	</actionset>
	
	<actionset key="RowSelected">
		<OperationString1>RowSelected</OperationString1>
	</actionset>
	
	<action key="FilterAdded">
		<SenderOperation>event</SenderOperation>
		<OperationString1>FilterAdded</OperationString1>
	</action>
	
	<action key="FilterRemoved">
		<SenderOperation>event</SenderOperation>
		<OperationString1>FilterRemoved</OperationString1>
	</action>
	
	<action key="RowSelected">
		<SenderOperation>event</SenderOperation>
		<OperationString1>RowSelected</OperationString1>
	</action>
	
</data-set>